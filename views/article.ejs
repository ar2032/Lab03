<%- include('header'); -%>
    <div class="content">
        <h1>Selected articles</h1>
        <table id="catalog">
            <thead>
                <tr>
                    <th>Name</th>
                    <th scope="col">Reference</th>
                    <th scope="col">Category</th>
                    <th scope="col">Price</th>
                    <th scope="col">supplier</th>
                    <th scope="col">vat</th>
                    <th scope="col">stock</th>
                    <th scope="col">critical_stock</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <th scope="row">
                        <%= products.name %>
                    </th>
                    <td>
                         <%= products.reference %>
                        </a>
                    </td>
                    <td>
                        <%= products.category %>
                    </td>
                    <td>
                        <%= products.price %>AED
                    </td>
                    <td>
                        <%= products.supplier %>
                    </td>
                    <td>
                        <%= products.vat %>
                    </td>
                    <td>
                        <%= products.stock %>
                    </td>
                    <td>
                        <%= products.critical_stock %>
                    </td>
                </tr>
        </tbody>
    </table>
</div>
<br>
<%- include('footer'); -%>
<%- include('header'); -%>
    <div class="content">
        <h1>client information</h1>
        <table id="catalog">
            <thead>
                <tr>
                    <th>ID</th>
                    
                    <th scope="col">Contact</th>
                    
                    <th scope="col">Phone</th>
                   
                </tr>
            </thead>
            <tbody>
                <% clients.forEach(client =>{ %>
                <tr>
                    <th scope="row">
                        <%= client.num_client %>
                    </th>
                    <td>
                        <%= client.contact %>
                    </td>
                    <td>
                        <%= client.phone %>
                    </td>
                </tr>
                <% }) %>
        </tbody>
    </table>
</div>
<br>
<%- include('footer'); -%>